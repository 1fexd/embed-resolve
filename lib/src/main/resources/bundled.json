{
  "services": [
    {
      "name": "Twitter",
      "domain":  "twitter.com",
      "pattern": "^(\\/\\w+\\/status\\/\\d+).*$",
      "embed_domains": ["vxtwitter.com", "fxtwitter.com", "twittpr.com"]
    },
    {
      "name": "X",
      "domain": "x.com",
      "pattern": "^(\\/\\w+\\/status\\/\\d+).*$",
      "embed_domains": ["fixvx.com", "fixupx.com"]
    },
    {
      "name": "Instagram",
      "domain": "instagram.com",
      "pattern": "^(\\/p\\/\\w+).*$",
      "embed_domains": ["ddinstagram.com"]
    },
    {
      "name": "Reddit",
      "domain": "reddit.com",
      "pattern": "^(\\/.*)$",
      "embed_domains": ["rxddit.com"]
    },
    {
      "name": "Pixiv",
      "domain": "pixiv.net",
      "pattern": "^(\\/.*)$",
      "ignore_pattern": "^(\\/api\\/info.*)$",
      "embed_domains": ["phixiv.net"]
    }
  ]
}
